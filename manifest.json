{
  "manifest_version": 3,
  "name": "Focus Time Manager",
  "version": "1.0",
  "description": "Track time and block distracting websites.",
  "permissions": [
    "storage",
    "tabs",
    "webNavigation",
    "alarms"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "options_page": "options.html"
}
